    "styles": {
        "blocks": {
            "core/archives": {
                "typography": {
                    "lineHeight": "1.5"
                }
            },
            "core/calendar": {
                "color": {
                    "text": "var(--wp--preset--color--contrast)"
                },
                "elements": {
                    "link": {
                        ":hover": {
                            "color": {
                                "text": "var(--wp--preset--color--accent-1)"
                            }
                        },
                        "color": {
                            "text": "var(--wp--preset--color--contrast)"
                        }
                    }
                },
                "typography": {
                    "fontSize": "var(--wp--preset--font-size--small)",
                    "fontStyle": "normal",
                    "fontWeight": "500",
                    "lineHeight": "1.8"
                }
            },
            "core/latest-posts": {
                "typography": {
                    "lineHeight": "1.5"
                }
            },
            "core/page-list": {
                "typography": {
                    "lineHeight": "1.5"
                }
            },
            "core/paragraph": {
                "typography": {
                    "lineHeight": "1.5"
                }
            },
            "core/verse": {
                "typography": {
                    "lineHeight": "1.6"
                }
            },
            "woocommerce/product-categories": {
                "typography": {
                    "lineHeight": "1.5"
                }
            }
        },
        "color": {
            "background": "var(--wp--preset--color--base)",
            "text": "var(--wp--preset--color--contrast)"
        },
        "elements": {
            "button": {
                "color": {
                    "background": "var(--wp--preset--color--accent-1)",
                    "text": "var(--wp--preset--color--base)"
                },
                ":hover": {
                    "color": {
                        "background": "color-mix( in lab, currentColor, var(--wp--preset--color--accent-1) 70% )",
                        "text": "var(--wp--preset--color--base)"
                    }
                }
            },
            "heading": {
                "color": {
                    "text": "var(--wp--preset--color--accent-1)"
                }
            },
            "link": {
                ":hover": {
                    "color": {
                        "text": "color-mix( in lab, currentColor, var(--wp--preset--color--accent-1) 70% )"
                    }
                },
                "color": {
                    "text": "var(--wp--preset--color--accent-1)"
                }
            }
        },
        "spacing": {
            "blockGap": "var(--wp--preset--spacing--40)",
            "padding": {
                "bottom": "0px",
                "left": "var(--wp--preset--spacing--50)",
                "right": "var(--wp--preset--spacing--50)",
                "top": "0px"
            }
        },
        "typography": {
            "fontFamily": "var(--wp--preset--font-family--system-font)"
        }
    },
    "templateParts": [
        {
            "area": "header",
            "name": "header"
        },
        {
            "area": "footer",
            "name": "footer"
        }
    ],
    "version": 3,
    "$schema": "https://schemas.wp.org/trunk/theme.json"
