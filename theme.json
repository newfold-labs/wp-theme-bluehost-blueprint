{
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1340px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"name": "X-Small",
					"size": "clamp(0.625rem, calc(0.5rem + 0.37vw), 0.875rem)",
					"slug": "20"
				},
				{
					"name": "Small",
					"size": "clamp(0.9375rem, calc(0.8125rem + 0.37vw), 1.25rem)",
					"slug": "30"
				},
				{
					"name": "Regular",
					"size": "clamp(1.25rem, calc(1.125rem + 0.61vw), 1.75rem)",
					"slug": "40"
				},
				{
					"name": "Large",
					"size": "clamp(1.75rem, calc(1.5rem + 0.61vw), 2.5rem)",
					"slug": "50"
				},
				{
					"name": "X-Large",
					"size": "clamp(2.1875rem, calc(1.9375rem + 1.14vw), 3.5rem)",
					"slug": "60"
				},
				{
					"name": "2X-Large",
					"size": "clamp(3rem, calc(2.5rem + 1.83vw), 5rem)",
					"slug": "70"
				},
				{
					"name": "3X-Large",
					"size": "clamp(4rem, calc(3.25rem + 2.44vw), 7rem)",
					"slug": "80"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"dropCap": true,
			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				}
			],
			"textColumns": true,
			"writingMode": true
		},
		"useRootPaddingAwareAlignments": true
	},
	"styles": {
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--40)",
			"padding": {
				"bottom": "0px",
				"left": "var(--wp--preset--spacing--50)",
				"right": "var(--wp--preset--spacing--50)",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--system-font)"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 3,
	"$schema": "https://schemas.wp.org/trunk/theme.json"
}