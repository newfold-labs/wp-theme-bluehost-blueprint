{
	"blocks": {
		"core/archives": {
			"typography": {
				"lineHeight": "1.5"
			}
		},
		"core/avatar": {
			"border": {
				"radius": "1000px"
			}
		},
		"core/calendar": {
			"color": {
				"text": "var(--wp--preset--color--contrast)"
			},
			"elements": {
				"link": {
					":hover": {
						"color": {
							"text": "var(--wp--preset--color--accent-1)"
						}
					},
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				}
			},
			"typography": {
				"fontSize": "var(--wp--preset--font-size--small)",
				"fontStyle": "normal",
				"fontWeight": "500",
				"lineHeight": "1.8"
			}
		},
		"core/code": {
			"color": {
				"background": "var(--wp--preset--color--base-midtone)"
			},
			"spacing": {
				"padding": {
					"bottom": "var(--wp--preset--spacing--20)",
					"left": "var(--wp--preset--spacing--20)",
					"right": "var(--wp--preset--spacing--20)",
					"top": "var(--wp--preset--spacing--20)"
				}
			},
			"typography": {
				"fontSize": "var(--wp--preset--font-size--small)"
			}
		},
		"core/latest-posts": {
			"typography": {
				"lineHeight": "1.5"
			}
		},
		"core/list": {
			"typography": {
				"lineHeight": "1.5"
			}
		},
		"core/list-item": {
			"spacing": {
				"margin": {
					"top": "0.8rem"
				}
			}
		},
		"core/page-list": {
			"typography": {
				"lineHeight": "1.5"
			}
		},
		"core/paragraph": {
			"typography": {
				"lineHeight": "1.5"
			}
		},
		"core/post-excerpt": {
			"color": {
				"text": "var(--wp--preset--color--contrast-midtone)"
			}
		},
		"core/post-terms": {
			"elements": {
				"link": {
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"core/separator": {
			"css": " &:not(.is-style-wide):not(.is-style-dots):not(.alignwide):not(.alignfull){width: 120px}"
		},
		"core/verse": {
			"typography": {
				"lineHeight": "1.6"
			}
		},
		"woocommerce/product-categories": {
			"typography": {
				"lineHeight": "1.5"
			}
		}
	}
}