{
    "elements": {
			"button": {
				"color": {
					"background": "var(--wp--preset--color--accent-1)",
					"text": "var(--wp--preset--color--base)"
				},
				":hover": {
					"color": {
						"background": "color-mix( in lab, currentColor, var(--wp--preset--color--accent-1) 70% )",
						"text": "var(--wp--preset--color--base)"
					}
				}
			},
			"heading": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				"typography": {
					"lineHeight": "1.1"
				}
			},
			"link": {
				":hover": {
					"color": {
						"text": "color-mix( in lab, currentColor, var(--wp--preset--color--accent-1) 70% )"
					}
				},
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"caption": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			}
		}
}